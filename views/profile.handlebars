<section class="dashboard">
  <h2>Profile</h2>
  <div class="">
    <div class="row flex-column">
      <div class="col">
        <span>ID:</span> {{profile.[0].id}}
      </div>
      <div class="col">
        <span>Username:</span> {{profile.[0].username}}
      </div>
      {{#ifGuest profile.[0].role.role}}
      <div class="col">
        <span>Email:</span> {{profile.[0].email}}
      </div>
      <div class="col">
        <span>Password:</span> *********
      </div>
      {{/ifGuest}}
      <div class="col">
        <span>Account Role:</span> {{profile.[0].role.role}}
      </div>
    </div>
  </div>
</section>