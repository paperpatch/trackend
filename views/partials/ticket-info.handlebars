<tr>
  <td>{{#if status}}Open{{else}}Closed{{/if}}</td>
  <td><a href="/ticket/{{id}}">{{id}}</a></td>
  <td><a href="/ticket/{{id}}">{{title}}</a></td>
  <td>{{type.type}}</td>
  <td>{{priority.level}}</td>
  <td>{{assign.username}}</td>
  <td>{{format_date due_date}}</td>
  <td><a href="/dashboard/edit/{{id}}" class="edit-link">Edit</a></td>
</tr>